nums = [1,2,4,6]
let res =[];


postfix =1 

// first for loop
prefix = 1 
    i =0  , 
    res[0] = 1 , 
    prefix = 1*1 = 1
    [1,2,4,6]

prefix = 1 
i=1 , res[1] = 1 , prefix = 2

[1,1,2]

prefix = 2 , i=2 , res[2] = 2 , prefix = 2*4 = 8
[1,1,2,6]

prefix = 8 , i=3 , res[3] = 2 , prefix = 2*4 = 8
[1,1,2,6]


for (let i = nums.length - 2; i >= 0; i--) {
    postfix *= nums[i+1];
    res[i] *= postfix;
}

postfix = 1 
i = 2 , postfix = nums[2+1] = 6 , res[2] = 2*6 = 12

[1,1,12,6]










